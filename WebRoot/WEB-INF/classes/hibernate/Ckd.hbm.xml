<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping package="com.chinasoft.domain">
    	<class name="Ckd" table="TB_CKD">
    		<id name="ckdId" column="ckdId">
    			<generator class="native"/>
    		</id>
    		 <property name="ckdNum" column="ckdNum" type="string" length="64"/>
    		 <property name="jsr" column="jsr" type="string" length="64"/>
    		 <property name="dh" column="dh" type="string" length="64"/>
    		 <property name="dz" column="dz" type="string" length="64"/>
    		 <property name="bz" column="bz" type="string" length="64"/>
    	</class>
    </hibernate-mapping>